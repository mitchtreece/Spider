        "PromiseKit/CorePromise": [
          "~> 6.0"
        ]
      }
    },
    {
      "name": "UIKit",
      "source_files": "Sources/Spider/UI/UIKit/**/*",
      "dependencies": {
        "Spider-Web/Core": [

        ],
        "Espresso/UIKit": [
          "~> 3.0"
        ],
        "Kingfisher": [
          "~> 7.0"
        ]
      }
    },
    {
      "name": "UIKit-PromiseKit",
      "source_files": "Sources/Spider/UI/UIKit-PromiseKit/**/*",
      "dependencies": {
        "Spider-Web/UIKit": [

        ],
        "Spider-Web/PromiseKit": [

        ]
      }
    },
    {
      "name": "All",
      "dependencies": {
        "Spider-Web/Core": [

        ],
        "Spider-Web/UIKit": [

        ],
        "Spider-Web/UIKit-PromiseKit": [

        ],
        "Spider-Web/PromiseKit": [

        ]
      }
    }
  ],
