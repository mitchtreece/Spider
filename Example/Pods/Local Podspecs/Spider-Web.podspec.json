          "~> 3.0"
        ],
        "ReachabilitySwift": [
          "~> 5.0"
        ]
      }
    },
    {
      "name": "PromiseKit",
      "source_files": "Sources/Spider/PromiseKit/**/*",
      "dependencies": {
        "Spider-Web/Core": [

